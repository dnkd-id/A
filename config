proxies:
proxy-groups:
- name: GLOBAL
  type: select
  disable-udp: false
  proxies:
  - BESTPING
  use:
  - GIT
  url: http://www.gstatic.com/generate_204
  interval: '120'
- name: BESTPING
  type: url-test
  disable-udp: false
  use:
  - GIT
  url: http://www.gstatic.com/generate_204
  interval: '300'
proxy-providers:
  GIT:
    type: http
    url: ""
    path: "./proxy_provider/AKUN-GIT.yaml"
    interval: 300
    health-check:
      enable: true
      url: http://www.gstatic.com/generate_204
      interval: 300
rules:
  - MATCH,GLOBAL
